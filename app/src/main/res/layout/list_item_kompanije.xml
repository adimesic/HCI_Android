<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Kompanija ID"
        android:id="@+id/textViewKompanijaID"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Naziv"
        android:id="@+id/textViewNaziv"
        android:layout_below="@+id/textViewKompanijaID"
        android:layout_alignEnd="@+id/textViewKompanijaID"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="ID Broj"
        android:id="@+id/textViewIDBroj"
        android:layout_below="@+id/textViewNaziv"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/textViewNaziv" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Odgovorna osoba"
        android:id="@+id/textViewOdgovornaOsoba"
        android:layout_below="@+id/textViewIDBroj" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Email"
        android:id="@+id/textViewEMail"
        android:layout_below="@+id/textViewOdgovornaOsoba"
        android:layout_alignEnd="@+id/textViewOdgovornaOsoba"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Telefon"
        android:id="@+id/textViewTelefon"
        android:layout_below="@+id/textViewEMail"
        android:layout_alignEnd="@+id/textViewEMail"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Faks"
        android:id="@+id/textViewFaks"
        android:layout_below="@+id/textViewTelefon"
        android:layout_alignEnd="@+id/textViewTelefon"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Adresa"
        android:id="@+id/textViewAdresa"
        android:layout_below="@+id/textViewFaks"
        android:layout_alignEnd="@+id/textViewFaks"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Grad"
        android:id="@+id/textViewGrad"
        android:layout_below="@+id/textViewAdresa"
        android:layout_alignEnd="@+id/textViewAdresa"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="KompanijaID"
        android:id="@+id/textViewKompanijIDValue"
        android:layout_alignTop="@+id/textViewKompanijaID"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Naziv"
        android:id="@+id/textViewNazivValue"
        android:layout_below="@+id/textViewKompanijIDValue"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="IDBROJ"
        android:id="@+id/textViewIDBrojValue"
        android:layout_below="@+id/textViewNazivValue"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ime i prezime"
        android:id="@+id/textViewOdgovornaOsobaValue"
        android:layout_alignTop="@+id/textViewOdgovornaOsoba"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="email"
        android:id="@+id/textViewEMailValues"
        android:layout_below="@+id/textViewOdgovornaOsobaValue"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Telefon"
        android:id="@+id/textViewTelefonValue"
        android:layout_below="@+id/textViewEMailValues"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Faks"
        android:id="@+id/textViewFaksValues"
        android:layout_below="@+id/textViewTelefonValue"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Adresa"
        android:id="@+id/textViewAdresaValue"
        android:layout_alignTop="@+id/textViewAdresa"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Grad"
        android:id="@+id/textViewGradValue"
        android:layout_below="@+id/textViewAdresaValue"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
